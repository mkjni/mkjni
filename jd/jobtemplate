<script>
// Function to share on WhatsApp
document.getElementById('whatsappIcon').addEventListener('click', function(event) {
  event.preventDefault(); // Prevent default behavior of anchor tag
  var url = encodeURIComponent('https://www.jobnexusindia.com/jd/jobidofthejob.html');
  var shareUrl = 'https://api.whatsapp.com/send?text=' + url;
  // Open WhatsApp share URL in a new tab
  window.open(shareUrl, '_blank');
});

// Function to copy the link
document.getElementById('copyLinkIcon').addEventListener('click', function(event) {
  event.preventDefault(); // Prevent default behavior of anchor tag
  var url = 'https://www.jobnexusindia.com/jd/jobidofthejob.html';
  navigator.clipboard.writeText(url)
    .then(function() {
      alert('Link copied to clipboard');
    })
    .catch(function(error) {
      console.error('Failed to copy: ', error);
    });
});
</script>

